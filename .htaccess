RewriteEngine On
RewriteRule ^favicon.ico - [L]
RewriteRule ^robots.txt - [L]

RewriteCond %{REQUEST_METHOD} !(POST)
#RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)/$ /$1 [L,R=301]

RewriteCond %{HTTP_HOST} ^twtconsult.ru$ [OR]
RewriteCond %{HTTP_HOST} ^www.twtconsult.ru$
RewriteRule ^(.*)$ http\:\/\/www\.twtconsult\.com\/$1 [R=301,L]

#**********************************************************
#Rewriting everything to index.php                        *
#Be free to add any rewrite rules what you need above it. *
#                                                         *
RewriteRule .* index.php                                  
#**********************************************************