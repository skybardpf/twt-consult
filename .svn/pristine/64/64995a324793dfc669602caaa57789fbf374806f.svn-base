app: twt
title: twt_title
charset: utf-8
#mode: development # If set this mode will be used
#default_mode: development
load_named_conf: 0
db_prefix: ''
jquery_version: '-1.8.2.min'
mvc:
  dirs:
    models: models
    controllers: controllers
    views: views
  use_subdirs:
    controllers: 0
    models: 0
    views: 1

views:
  forms:
    label: '<label for="{id}">{title}</label>'
use_smarty: 1
smarty:
  compile_dir: .zf_compiled/

kcaptcha:
  bgcolor: '255-255-255'

page:
  extract_variables: 1
  extract_forms: 1

default_controller: content
has_common_controller: 1
vars_keys: [id, pid, page, pos, application, god_mode, sort, dir, ajax]

session:
  save_path: .zf_tmp
  autostart: 1
  lifetime: 0
  update_lifetime: 0
  name: twt_session

